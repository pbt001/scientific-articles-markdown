  year = {2009},
  keywords = {liquid chromatography-mass spectrometry,MS/MS spectral tag,non-targeted metabolic profiling analysis,peak annotation,secondary metabolites},
  pages = {555--577},
  file = {Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/6R2UINUX/Matsuda et al. - 2009 - MSMS spectral tag-based annotation of non-targete.pdf:application/pdf;Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/A8RTWH8A/Matsuda et al. - 2009 - MSMS spectral tag-based annotation of non-targete.pdf:application/pdf;Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/G9D52PU4/Matsuda et al. - 2009 - MSMS spectral tag-based annotation of non-targete.pdf:application/pdf;Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/9F3PZEKG/abstract.html:text/html;Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/C3RI987S/abstract.html:text/html;Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/QJE3HFR2/full.html:text/html}
