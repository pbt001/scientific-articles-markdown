  pages = {D655--D658},
  file = {Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/DPI32ZBQ/Desiere et al. - 2006 - The PeptideAtlas project.pdf:application/pdf;Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/8KXEDP9J/D655.html:text/html},
