@article{bocklitz_deeper_2013,
  title = {Deeper {{Understanding}} of {{Biological Tissue}}: {{Quantitative Correlation}} of {{MALDI}}-{{TOF}} and {{Raman Imaging}}},
  volume = {85},
  issn = {0003-2700},
  shorttitle = {Deeper {{Understanding}} of {{Biological Tissue}}: {{Quantitative Correlation}} of {{MALDI}}-{{TOF}} and {{Raman Imaging}}},
  doi = {10.1021/ac402175c},
  abstract = {In order to achieve a comprehensive description of biological tissue, spectral information about proteins, lipids, nucleic acids, and other biochemical components need to be obtained concurrently. Different analytical techniques may be combined to record complementary information of the same sample. Established techniques, which can be utilized to elucidate the biochemistry of tissue samples are, for instance, MALDI-TOF-MS and Raman microscopic imaging. With this contribution, we combine these two techniques for the first time. The combination of both techniques allows the utilization and interpretation of complementary information (i.e., the information about the protein composition derived from the Raman spectra with data of the lipids analyzed by the MALDI-TOF measurements). Furthermore, we demonstrate how spectral information from MALDI-TOF experiments can be utilized to interpret Raman spectra.},
  language = {English},
  timestamp = {2016-09-13T15:47:37Z},
  number = {22},
  journal = {Anal. Chem.},
  author = {Bocklitz, T. W. and Creceius, A. C. and Matthaus, C. and Tarcea, N. and {von Eggeling}, F. and Schmitt, M. and Schubert, U. S. and Popp, J.},
  month = nov,
  year = {2013},
  keywords = {cancer diagnosis,cells,mass-spectrometry,microspectroscopy,proteomic analysis,spectroscopy,tool},
  pages = {10829--10834}
