  timestamp = {2013-10-04T20:01:20Z},
  number = {7},
  journal = {Appl. Biochem. Biotechnol.},
  author = {Gonz{\'a}lez-R{\'a}bade, Nuria and Del Carmen Oliver-Salvador, Mar{\'\i}a and Salgado-Manjarrez, Edgar and Badillo-Corona, Jes{\'u}s Agust{\'\i}n},
  month = apr,
