  keywords = {Aroma,Food safety,Gas chromatography–olfactometry (GC–O),Green coffee,Post-harvest process,Solid-phase microextraction (SPME)},
  pages = {289--296},
  file = {ScienceDirect Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/FR3BSXRM/S0889157506001529.html:text/html}
