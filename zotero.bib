  urldate = {2015-10-23},
  journal = {Aging Cell},
  author = {Laye, Matthew J. and Tran, ViLinh and Jones, Dean P. and Kapahi, Pankaj and Promislow, Daniel E. L.},
  month = oct,
  year = {2015},
  keywords = {Aging,Bioinformatics,dietary restriction,Drosophila,Metabolome,Metabolomics},
  pages = {797--808},
  file = {Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/DZRVBUII/Laye et al. - 2015 - The effects of age and dietary restriction on the .pdf:application/pdf;Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/XCMGCA9E/full.html:text/html}
}

