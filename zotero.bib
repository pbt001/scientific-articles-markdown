  month = aug,
  year = {2012},
  keywords = {Diabetes,Diet,Drosophila melanogaster,insulin resistance,Insulin signaling},
  pages = {1230--1237},
  file = {ScienceDirect Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/JJJC4TAG/Morris et al. - 2012 - Development of diet-induced insulin resistance in .pdf:application/pdf;ScienceDirect Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/NWSPEXCD/S0925443912000993.html:text/html}
}

