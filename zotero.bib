  month = oct,
  year = {2014},
  keywords = {Analytical Chemistry,Biochemistry; general,Characterization and Evaluation of Materials,Cloud computing,Environmental Monitoring/Analysis,Food Science,FTMS,Imaging mass spectrometry,Laboratory Medicine,MALDI,Parallel processing,Supercomputing},
  pages = {2321--2327},
  file = {Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/QARCDJ4U/Smith et al. - 2014 - Distributed computing strategies for processing of.pdf:application/pdf;Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/7336FZWN/s00216-014-8210-0.html:text/html}
