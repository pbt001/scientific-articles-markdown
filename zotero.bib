@article{eliasson_ozone_1987,
  title = {Ozone Synthesis from Oxygen in Dielectric Barrier Discharges},
  volume = {20},
  issn = {0022-3727},
  abstract = {A comprehensive model of ozone generation in dielectric barrier discharges is presented. The model
