  keywords = {Algorithms,Cell Line; Tumor,Gas Chromatography-Mass Spectrometry,Glutamine,Humans,Isotope Labeling,Metabolic Networks and Pathways,Metabolomics},
  pages = {6621--6628},
  file = {ACS Full Text PDF w/ Links:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/SVHIKHBZ/Hiller et al. - 2010 - Nontargeted Elucidation of Metabolic Pathways Usin.pdf:application/pdf;ACS Full Text Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/5IFUCGQI/ac1011574.html:text/html}
