  timestamp = {2016-09-22T16:32:22Z},
  number = {3},
  urldate = {2016-05-04},
  journal = {Am J Hypertens},
  author = {Pierdomenico, Sante D. and Pierdomenico, Anna M. and Tommaso, Roberta Di and Coccina, Francesca and Carlo, Silvio Di and Cuccurullo, Franco and Porreca, Ettore},
  month = jan,
  year = {2016},
  keywords = {blood pressure,coronary events,elderly,Hypertension,metabolic syndrome,stroke.},
  pages = {365--371},
  file = {Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/6KRW4TK6/Pierdomenico et al. - 2016 - Metabolic Syndrome and Cardiovascular Risk in Elde.pdf:application/pdf;Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/9W34D24Q/Pierdomenico et al. - 2016 - Metabolic Syndrome and Cardiovascular Risk in Elde.pdf:application/pdf;Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/IP8Q3UDX/365.html:text/html;Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/S3DRFQF2/365.html:text/html},
