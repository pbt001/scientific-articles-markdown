  month = jan,
  year = {2011},
  keywords = {acylation,amidation,dimethylation,Mass spectrometry (MS),Peptide derivatization,protein derivatization,Protein Science,Proteomics,stable isotope labeling},
  pages = {77--91},
  file = {Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/V4KHHTCJ/Krusemark et al. - 2011 - Complete Chemical Modification of Amine and Acid F.pdf:application/pdf;Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/6NGS55P7/10.html:text/html}
