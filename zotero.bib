@article{wang_structure_2011,
  title = {Structure Elucidation and Toxicity Analyses of the Radiolytic Products of Aflatoxin {{B1}} in Methanol\textendash{}water Solution},
  volume = {192},
  issn = {0304-3894},
  doi = {10.1016/j.jhazmat.2011.06.027},
  abstract = {The identification of the radiolytic products of mycotoxins is a key issue in the feasibility study of gamma ray radiation detoxification. Methanol\textendash{}water solution (60:40, v/v) spiked with aflatoxin B1 (AFB1; 20 mg L-1) was irradiated with Co60 gamma ray to generate radiolytic products. Liquid chromatography\textendash{}quadruple time-of-flight mass spectrometry was applied to identify the radiolytic products of AFB1. Accurate mass and proposed molecular formulas with a high-matching property of more than 20 radiolytic products were obtained. Seven key radiolytic products were proposed based on the molecular formulas and tandem mass spectrometry spectra. The analyses of toxicity and formation pathways were proposed based on the structure of the radiolytic products. The addition reaction caused by the free-radical species in the methanol\textendash{}water solution resulted in the formation of most radiolytic products. Based on the structure\textendash{}activity relationship analysis, the toxicity of radiolytic products was significantly reduced compared with that of AFB1 because of the addition reaction that occurred on the double bond in the terminal furan ring. For this reason, gamma irradiation is deemed an effective tool for the detoxification of AFB1.},
  timestamp = {2014-01-22T14:36:45Z},
  number = {3},
  urldate = {2014-01-22},
  journal = {Journal of Hazardous Materials},
  author = {Wang, Feng and Xie, Fang and Xue, Xiaofeng and Wang, Zhidong and Fan, Bei and Ha, Yiming},
  month = sep,
  year = {2011},
  keywords = {Aflatoxin B1,LC–Q-TOF MS,Radiolytic products,structure,Toxicity},
  pages = {1192--1202},
  file = {ScienceDirect Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/2IEXNW44/S0304389411007898.html:text/html}
}

@article{patti_meta-analysis_2012,
  title = {Meta-Analysis of Untargeted Metabolomic Data from Multiple Profiling Experiments},
  volume = {7},
  copyright = {\textcopyright{} 2012 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
  issn = {1754-2189},
  doi = {10.1038/nprot.2011.454},
  abstract = {metaXCMS is a software program for the analysis of liquid chromatography/mass spectrometry\textendash{}based untargeted metabolomic data. It is designed to identify the differences between metabolic profiles across multiple sample groups (e.g., 'healthy' versus 'active disease' versus 'inactive disease'). Although performing pairwise comparisons alone can provide physiologically relevant data, these experiments often result in hundreds of differences, and comparison with additional biologically meaningful sample groups can allow for substantial data reduction. By performing second-order (meta-) analysis, metaXCMS facilitates the prioritization of interesting metabolite features from large untargeted metabolomic data sets before the rate-limiting step of structural identification. Here we provide a detailed step-by-step protocol for going from raw mass spectrometry data to metaXCMS results, visualized as Venn diagrams and exported Microsoft Excel spreadsheets. There is no upper limit to the number of sample groups or individual samples that can be compared with the software, and data from most commercial mass spectrometers are supported. The speed of the analysis depends on computational resources and data volume, but will generally be less than 1 d for most users. metaXCMS is freely available at http://metlin.scripps.edu/metaxcms/.},
  language = {en},
  timestamp = {2014-01-21T17:28:32Z},
  number = {3},
  urldate = {2014-01-21},
  journal = {Nat. Protocols},
  author = {Patti, Gary J. and Tautenhahn, Ralf and Siuzdak, Gary},
  month = mar,
  year = {2012},
  keywords = {Computational Biology,Metabolomics},
  pages = {508--516},
  file = {Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/BSPBAAEJ/nprot.2011.454.html:text/html}
}

