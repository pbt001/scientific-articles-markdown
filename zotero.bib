  year = {2007},
  keywords = {Lipidomics,Mass Spectrometry,metabolic fingerprinting,metabolic profiling,Metabolomics},
  pages = {51--78},
  file = {Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/X5TJ9H6X/Dettmer et al. - 2007 - Mass spectrometry-based metabolomics.pdf:application/pdf;Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/XZ9UGMRV/full.html:text/html}
