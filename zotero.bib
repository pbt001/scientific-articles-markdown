  year = {2004 Sep-Oct},
  keywords = {Algorithms,Computational Biology,Databases; Protein,False Positive Reactions,Mass Spectrometry,Peptide Fragments,Poisson Distribution,Proteins,Proteomics,Reproducibility of Results,ROC Curve},
  pages = {958--964},
