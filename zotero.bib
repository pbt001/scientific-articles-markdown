  pages = {1775--1782},
  file = {Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/PS9V85I2/Groth et al. - 2004 - Construction of Transgenic Drosophila by Using the.pdf:application/pdf;Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/VBUADCXV/1775.html:text/html},
  pmid = {15126397}
}

@book{markow_drosophila:_2005,
  address = {Amsterdam ; Boston},
  title = {Drosophila: {{A Guide}} to {{Species Identification}} and {{Use}}},
