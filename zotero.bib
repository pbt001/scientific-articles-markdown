  month = may,
  year = {2012},
  keywords = {Drosophila,Genomic modification,Reverse genetics,TALEN,Unit assembly},
  pages = {209--215},
  file = {ScienceDirect Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/4EU3S6TQ/Liu et al. - 2012 - Efficient and Specific Modifications of the Drosop.pdf:application/pdf;ScienceDirect Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/3FCCB3N7/S167385271200077X.html:text/html}
}

