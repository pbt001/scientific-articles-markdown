  journal = {J. Mass Spectrom.},
  author = {King, F. L. and Teng, J. and Steiner, R. E.},
  month = aug,
  year = {1995},
  pages = {1061--1075},
  file = {Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/23U3RARP/King et al. - 1995 - Special feature Tutorial. Glow discharge mass spe.pdf:application/pdf;Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/8XPXFN9Z/abstract.html:text/html}
}

@article{winter_spatio-temporal_2015,
  title = {The Spatio-Temporal Distribution of {{He}} (2 3 {{S}} 1 ) Metastable Atoms in a {{MHz}}-Driven Helium Plasma Jet Is Influenced by the Oxygen/Nitrogen Ratio of the Surrounding Atmosphere},
  volume = {24},
  issn = {0963-0252},
  doi = {10.1088/0963-0252/24/2/025015},
  abstract = {The density of helium He (2 3 S 1 ) metastable atoms is measured in a 1.6 mm diameter MHz-driven atmospheric pressure helium plasma jet by laser absorption spectroscopy with spatial and temporal resolution. The surrounding atmosphere of the jet is varied from pure oxygen to pure nitrogen with a gas shielding device. The highest metastable density of 1.3 \texttimes{} 10 13 cm -3 is obtained in the center of the jet close to the nozzle exit at normal atmospheric air conditions. Within 0.3 mm in the radial direction and 2 mm in the axial direction, the He metastable density drops below the detection limit. The obtained He metastable lifetime is almost independent of the shielding gas composition. By analyzing the diffusion of shielding gas species into the effluent it is concluded that their density is too low to explain the observed He metastable lifetime. Instead, impurities from the feed gas, especially water molecules, are more likely to be responsible. However, a drastic change in metastable He density is observed when decreasing the amount of oxygen in the shielding gas. The lower the oxygen amount, the lower the metastable He density. For pure nitrogen, no He metastables are detected at all. By exchanging nitrogen with argon, a similar behavior is observed. Thus, it is concluded that it is the absence of ambient oxygen rather than the elevated presence of nitrogen, which is responsible for the observed decrease in the He (2 3 S 1 ) density.},
  language = {en},
  timestamp = {2016-09-27T17:43:20Z},
  number = {2},
  urldate = {2016-09-27},
  journal = {Plasma Sources Sci. Technol.},
  author = {Winter, J. and Sousa, J. Santos and Sadeghi, N. and Schmidt-Bleker, A. and Reuter, S. and Puech, V.},
  year = {2015},
  pages = {025015},
  file = {IOP Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/BVCCI2IF/Winter et al. - 2015 - The spatio-temporal distribution of He (2 3 S 1 ) .pdf:application/pdf}
}

