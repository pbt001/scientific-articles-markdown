  keywords = {Algorithms,Alternative Splicing,Computational Biology,Databases; Protein,Data Interpretation; Statistical,Fourier Analysis,Genome; Human,Genomics,Humans,Mass Spectrometry,Neoplasm Proteins,Peptides,Probability,Sequence Analysis; Protein,Spectroscopy; Fourier Transform Infrared},
