@article{katajamaa_data_2007,
  series = {Data Analysis in Chromatography},
  title = {Data Processing for Mass Spectrometry-Based Metabolomics},
  volume = {1158},
  issn = {0021-9673},
  doi = {10.1016/j.chroma.2007.04.021},
  abstract = {Modern analytical technologies afford comprehensive and quantitative investigation of a multitude of different metabolites. Typical metabolomic experiments can therefore produce large amounts of data. Handling such complex datasets is an important step that has big impact on extent and quality at which the metabolite identification and quantification can be made, and thus on the ultimate biological interpretation of results. Increasing interest in metabolomics thus led to resurgence of interest in related data processing. A wide variety of methods and software tools have been developed for metabolomics during recent years, and this trend is likely to continue. In this paper we overview the key steps of metabolomic data processing and focus on reviewing recent literature related to this topic, particularly on methods for handling data from liquid chromatography mass spectrometry (LC\textendash{}MS) experiments.},
