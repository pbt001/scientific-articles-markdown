  keywords = {Ipython,Jupyter,MALDI,Mass spectrometry imaging,Metabolomics,Openmsi},
  pages = {18--26},
  file = {ScienceDirect Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/EEA2GDAQ/Fischer et al. - 2016 - An accessible, scalable ecosystem for enabling and.pdf:application/pdf;ScienceDirect Full Text PDF:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/KZAMVQFS/Fischer et al. - 2016 - An accessible, scalable ecosystem for enabling and.pdf:application/pdf;ScienceDirect Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/P4AHUNFB/S0003986115300473.html:text/html;ScienceDirect Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/R76CVHTN/S0003986115300473.html:text/html}
}

@article{woese_concept_1977,
  title = {The Concept of Cellular Evolution},
  volume = {10},
  issn = {0022-2844, 1432-1432},
  doi = {10.1007/BF01796132},
  abstract = {Summary A central evolutionary question is whether the eucaryotic cytoplasm represents a line of descent that is separate from the typical bacterial line. It is argued on the basis of differences between their respective translation mechanisms that the two lines do represent separate phylogenetic trees in the sense that each line of descent independently evolved to a level of organization that could be called procaryotic. The two lines of descent, nevertheless shared a common ancestor, that was far simpler than the procaryote. This primitive entity is called a progenote, to recognize the possibility that it had not yet completed evolving the link between genotype and phenotype. This concept changes considerably the view one takes toward cellular evolution.},
  language = {en},
  timestamp = {2016-09-22T16:54:29Z},
  number = {1},
  urldate = {2016-04-04},
