  keywords = {Calibration,Diagnostic Imaging,Equipment and Supplies,Humans,Mass Spectrometry,Software,Spectrometry; Mass; Electrospray Ionization,Spectrometry; Mass; Matrix-Assisted Laser Desorption-Ionization},
  pages = {4931--4940},
  pmid = {22820537}
}

@article{gusakov_mass_2010,
