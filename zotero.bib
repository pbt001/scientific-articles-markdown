@article{al-zereini_new_2007,
  title = {New Aromatic Nitro Compounds from {{Salegentibacter}} Sp {{T436}}, an {{Arctic Sea}} Ice Bacterium: {{Taxonomy}}, Fermentation, Isolation and Biological Activities},
