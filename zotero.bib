@article{prikryl_fluorescence_2014,
  title = {Fluorescence {{Detector}} for {{Capillary Separations Fabricated}} by {{3D Printing}}},
  issn = {0003-2700},
  doi = {10.1021/ac503678n},
  abstract = {A simple inexpensive light-emitting diode (LED)-based fluorescence detector for detection in capillary separations is described. The modular design includes a separate high power LED source, detector head, designed in the epifluorescence arrangement, and capillary detection cells. The detector head and detection cells were printed using a 3D printer and assembled with commercially available optical components. Optical fibers were used for connecting the detector head to the LED excitation source and the photodetector module. Microscope objective or high numerical aperture optical fiber were used for collection of the fluorescence emission from the fused silica separation capillary. As an example, mixture of oligosaccharides labeled by 8-aminopyrene-1,3,6-trisulfonate (APTS) was separated by capillary zone electrophoresis and detected by the described detector. The performance of the detector was compared with both a semiconductor photodiode and photomultiplier as light sensing elements. The main advantages of the 3D printed parts, compared to the more expensive alternatives from the optic component suppliers, include not only cost reduction, but also easy customization of the spatial arrangement, modularity, miniaturization, and sharing of information between laboratories for easy replication or further modifications of the detector. All information and files necessary for printing the presented detector are enclosed in the Supporting Information.},
  timestamp = {2014-12-09T20:18:23Z},
  urldate = {2014-12-09},
  journal = {Anal. Chem.},
  author = {Prikryl, Jan and Foret, Frantisek},
  year = {2014},
  file = {ACS Full Text PDF w/ Links:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/GPZQ5ZBX/Prikryl and Foret - 2014 - Fluorescence Detector for Capillary Separations Fa.pdf:application/pdf;ACS Full Text Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/C8MW78QQ/ac503678n.html:text/html}
}

@article{luo_low-voltage_2014,
  title = {Low-{{Voltage Origami}}-{{Paper}}-{{Based Electrophoretic Device}} for {{Rapid Protein Separation}}},
  issn = {0003-2700},
  doi = {10.1021/ac503976c},
  abstract = {We present an origami paper-based electrophoretic device (oPAD-Ep) that achieves rapid (?5 min) separation of fluorescent molecules and proteins. Due to the innovative design, the required driving voltage is just ?10 V, which is more than 10 times lower than that used for conventional electrophoresis. The oPAD-Ep uses multiple, thin (180 ?m/layer) folded paper layers as the supporting medium for electrophoresis. This approach significantly shortens the distance between the anode and cathode, and this, in turn, accounts for the high electric field ($>$1 kV/m) that can be achieved even with a low applied voltage. The multilayer design of the oPAD-Ep enables convenient sample introduction by use of a slip layer as well as easy product analysis and reclamation after electrophoresis by unfolding the origami paper and cutting out desired layers. We demonstrate the use of oPAD-Ep for simple separation of proteins in bovine serum, which illustrates its potential applications for point-of-care diagnostic testing.},
  timestamp = {2014-12-09T20:22:17Z},
  urldate = {2014-12-09},
  journal = {Anal. Chem.},
  author = {Luo, Long and Li, Xiang and Crooks, Richard M.},
  year = {2014},
  file = {ACS Full Text PDF w/ Links:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/QEPKE3XF/Luo et al. - 2014 - Low-Voltage Origami-Paper-Based Electrophoretic De.pdf:application/pdf;ACS Full Text Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/C982KU63/ac503976c.html:text/html}
}

