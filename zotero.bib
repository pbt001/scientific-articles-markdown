  keywords = {Algorithm,Isotope distribution,Mass Spectrometry},
  pages = {107--119},
  file = {ScienceDirect Snapshot:/home/rob/.mozilla/firefox/x4ox4i88.default/zotero/storage/X6IWAM8A/S0003267000830597.html:text/html}
}

